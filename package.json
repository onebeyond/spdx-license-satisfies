    "SPDX",
    "law",
    "legal",
    "license",
    "metadata",
    "package",
    "package.json",
    "standards"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/onebeyond/spdx-license-satisfies.git"
  },
  "bugs": {
    "url": "https://github.com/onebeyond/spdx-license-satisfies/issues"
  },
  "homepage": "https://github.com/onebeyond/spdx-license-satisfies#readme",
  "main": "src/index.js",
  "scripts": {
